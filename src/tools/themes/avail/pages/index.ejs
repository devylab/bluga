<%- await include('../components/header') %>
<%- await include('../components/navbar') %>

	<div class="container-fluid">
		<div class="row fh5co-post-entry">
      <% const { data: contents } = await service.content.getContents('DRAFT'); %>
      <% for(var i=0; i < contents.length; i++) { %>
        <article class="col-lg-3 col-md-3 col-sm-3 col-xs-6 col-xxs-12 animate-box">
          <figure>
            <a href="<%= contents[i].slug %>"><img src="<%= themePath() %>/images/pic_1.jpg" alt="Image" class="img-responsive"></a>
          </figure>
          <span class="fh5co-meta"><a href="<%= contents[i].slug %>">Food &amp; Drink</a></span>
          <h2 class="fh5co-article-title"><a href="<%= contents[i].slug %>"><%= contents[i].title %></a></h2>
          <span class="fh5co-meta fh5co-date">
						<%= new Date(contents[i].createdAt).toLocaleDateString("en-US", {month: "long",day: "numeric",year: "numeric"}) %>
					</span>
        </article>
      <% } %>
			<!-- <div class="clearfix visible-lg-block visible-md-block visible-sm-block visible-xs-block"></div>

			<div class="clearfix visible-xs-block"></div> -->
		</div>
	</div>
</main>
<%- await include('../components/footer') %>
